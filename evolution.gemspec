Gem::Specification.new do |spec|
  spec.name          = "Jekyll_Candor"
  spec.version       = "0.1.0"
  spec.authors       = ["Ted Martin"]
  spec.email         = ["etmartinkazoo@gmail.com"]

  spec.summary       = %q{A minimal Jekyll starter theme for developers}
  spec.homepage      = "https://www.tedmartin.us/candor"
  spec.license       = "MIT"

  spec.add_runtime_dependency "jekyll", "~> 4.2.0"

  spec.add_development_dependency "bundler", "~> 2.2.4"
  spec.add_development_dependency "rake", "~> 13.0.3"
end
